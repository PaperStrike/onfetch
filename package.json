{
  "name": "onfetch",
  "version": "0.1.1",
  "description": "Mock fetch() with native Request / Response API",
  "types": "./build/index.d.ts",
  "main": "./build/index.js",
  "type": "module",
  "browser": {
    "expect": "@sliphua/expect",
    "mocha": "mocha/mocha-es2018.js",
    "node-fetch": false
  },
  "exports": {
    ".": "./build/index.js",
    "./core": "./build/core.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "prepare": "npm run build",
    "test-browser": "playwright-test test/**/*.test.*",
    "test-node": "mocha --experimental-specifier-resolution=node",
    "test": "npm run test-browser && npm run test-node"
  },
  "files": [
    "src",
    "build",
    "tsconfig.json"
  ],
  "keywords": [
    "mock",
    "fetch",
    "test",
    "testing",
    "fake",
    "spy"
  ],
  "author": "PaperStrike",
  "repository": "github:PaperStrike/onfetch",
  "license": "ISC",
  "devDependencies": {
    "@sliphua/expect": "^27.2.0",
    "@types/mocha": "^9.0.0",
    "@types/web": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-plugin-import": "^2.24.2",
    "expect": "^27.2.0",
    "mocha": "^9.1.1",
    "node-fetch": "^3.0.0",
    "playwright-fixtures": "^0.3.4",
    "playwright-test": "^7.1.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
